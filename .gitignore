*.iml
/.idea/modules.xml
/.idea/libraries/*.xml
/.idea/copyright/profiles_settings.xml
project.properties
.idea/runConfigurations/Debug_Buck_test.xml
.idea/compiler.xml
.idea/encodings.xml
.idea/scopes/
.idea/vcs.xml

# Compiled Python
*.pyc
*py.class

# Temp
tmp
*.swp
*.swo

# IntelliJ, based on http://devnet.jetbrains.net/docs/DOC-1186
.idea/ant.xml
.idea/dictionaries
.idea/inspectionProfiles
.idea/tasks.xml
.idea/uiDesigner.xml
.idea/workspace.xml
.idea/misc.xml
.idea/runConfigurations

